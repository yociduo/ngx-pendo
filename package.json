{
  "name": "ngx-pendo-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-schematics": "tsc -p ./projects/ngx-pendo/schematics/tsconfig.json && jasmine ./projects/ngx-pendo/schematics/**/*.spec.js",
    "lint": "ng lint",
    "build-pkg": "ng build ngx-pendo --configuration production",
    "build-schematics": "tsc -p ./projects/ngx-pendo/schematics/tsconfig.json --outDir dist/ngx-pendo/schematics",
    "run-schematics": "schematics ./projects/ngx-pendo/schematics/dist/collection.json:ng-add",
    "gen-schema": "npx -p dtsgenerator dtsgen projects/ngx-pendo/schematics/ng-add/schema.json -o projects/ngx-pendo/schematics/ng-add/schema.d.ts",
    "cp-readme": "copyfiles ./README.md ./dist/ngx-pendo",
    "cp-schema": "copyfiles -f ./projects/ngx-pendo/schematics/ng-add/schema.json ./dist/ngx-pendo/schematics/ng-add",
    "cp-collection": "copyfiles -f ./projects/ngx-pendo/schematics/collection.json ./dist/ngx-pendo/schematics",
    "cp": "npm run cp-readme && npm run cp-schema && npm run cp-collection",
    "release": "cd ./projects/ngx-pendo && standard-version",
    "publish": "npm run build-pkg && npm run build-schematics && npm run cp && npm publish dist/ngx-pendo",
    "prepare": "husky install"
  },
  "author": "Eden Wang",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^15.0.0",
    "@angular-devkit/schematics": "^15.0.0",
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "change-case": "^3.1.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "^15.0.0",
    "@angular-eslint/eslint-plugin": "^15.0.0",
    "@angular-eslint/eslint-plugin-template": "^15.0.0",
    "@angular-eslint/schematics": "^15.0.0",
    "@angular-eslint/template-parser": "^15.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/language-service": "^15.0.0",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@schematics/angular": "^15.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "copyfiles": "^2.3.0",
    "dtsgenerator": "^3.6.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.0",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^11.0.0",
    "ng-packagr": "^15.0.0",
    "prettier": "^1.19.1",
    "protractor": "~7.0.0",
    "standard-version": "^9.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.8.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src|projects/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src|projects/**/*.{scss,less,css}": [
      "stylelint --fix"
    ],
    "src|projects/**/*.{ts,js,json,md,scss,less,css}": [
      "prettier --write --config .prettierrc"
    ]
  }
}
